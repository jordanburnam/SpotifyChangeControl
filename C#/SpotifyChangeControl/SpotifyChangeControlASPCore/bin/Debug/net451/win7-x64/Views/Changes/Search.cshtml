@using Mvc.Client.Models.Changes
@model SearchModel
@{
   
    ViewBag.Title = "Search";
}
<h1>@ViewBag.Title</h1>
<form action="Results" method="get">
    <p>Start:@Html.EditorFor(model => model.Start)</p>
    <p>End: @Html.EditorFor(model => model.End)</p>
     <p><button class="btn btn-lg btn-success" type="submit">Search</button></p>
  
</form>